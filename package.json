{
  "name": "deserve-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css:dev": "tailwindcss -i ./src/app/globals.css -o ./src/styles/tw-built.css --watch",
    "css:build": "tailwindcss -i ./src/app/globals.css -o ./src/styles/tw-built.css --minify",
    "predev:DISABLED": "node scripts/fix-next-build.js",
    "dev": "next dev",
    "dev:old": "concurrently -k \"npm:css:dev\" \"next dev\"",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --reporter=dot --testNamePattern='^(?!.*\\[integration\\])'",
    "test:unit": "vitest run --reporter=dot --testNamePattern='^(?!.*\\[integration\\])'",
    "test:int": "vitest run --reporter=dot --testNamePattern='\\[integration\\]'",
    "test:watch": "vitest --watch",
    "test:cov": "vitest run --coverage --testNamePattern='^(?!.*\\[integration\\])'",
    "clean": "rimraf .next node_modules && rm -f package-lock.json && npm i",
    "clean:soft": "rimraf .next && npm run dev",
    "clean:cache": "rimraf .next node_modules/.cache",
    "restart": "lsof -ti:3000 | xargs kill -9 2>/dev/null; rm -rf .next && npm run dev",
    "reinstall": "rm -f package-lock.json && rimraf node_modules && npm install",
    "dev:fresh": "npm run clean:cache && npm run dev",
    "typecheck": "tsc --noEmit",
    "gen:types": "node scripts/generate-db-types.mjs",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "ts:show": "tsc --showConfig",
    "ts:diag": "tsc --extendedDiagnostics --listFiles --noEmit",
    "qa:auth": "node ../scripts/qa-auth.mjs",
    "lint:paths": "node scripts/check-case-imports.mjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "mercadopago": "^2.9.0",
    "next": "14.2.32",
    "next-pwa": "^5.6.0",
    "openai": "^6.2.0",
    "react": "18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.8",
    "recharts": "^3.2.1",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "whatwg-url": "^15.1.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.32",
    "jsdom": "^27.0.0",
    "postcss": "^8",
    "rimraf": "^6.0.1",
    "source-map-js": "^1.0.2",
    "supabase": "^1.200.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
